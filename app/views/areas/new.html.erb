<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @area, local: true) do |form| %>
      <%= form.file_field :image, id:"area-image" %>  
      <%= form.collection_select(:prefecture_id, Prefecture.all, :id, :name, {}, {class:"select-box", id:"item-prefecture"}) %>
      <%= form.text_area :area_text, placeholder: "text", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>